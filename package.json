{
    "name": "go-front-libs",
    "version": "0.0.1",
    "description": "JS Libraries for GO generated apps",
    "author": "GenerativeObjects",
    "private": true,
    "devDependencies": {
        "babel": "^6.23.0",
        "babel-core": "^6.26.3",
        "babel-plugin-istanbul": "^1.1.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-register": "^6.26.0",
        "chai": "^4.1.2",
        "cross-env": "^5.1.6",
        "mocha": "^5.2.0",
        "mocha-webpack": "^1.1.0",
        "nyc": "^11.8.0",
        "webpack": "^3.6.0"
    },
    "dependencies": {
        "atob": "^2.1.1",
        "mersennetwister": "^0.2.3"
    },
    "scripts": {
        "test": "mocha --require babel-register --require babel-polyfill \"test/unit/**/*.js\"",
        "test-watch": "mocha-webpack --watch \"test/unit/**/*.js\"",
        "cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --require babel-polyfill \"test/unit/**/*.js\""
    },
    "nyc": {
        "all": false,
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "report-dir": "./test/coverage",
        "exclude": [
            "**/node_modules/**",
            ".tmp",
            "test"
        ],
        "extension": [
            "js"
        ],
        "cache": true,
        "sourceMap": false,
        "instrument": false
    }
}